    "debug": "~4.3.1",
    "dotenv": "^8.2.0",
    "event-stream": "^4.0.1",
    "express": "~4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.7.0",
    "http-errors": "~1.8.0",
    "morgan": "~1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.8.0",
    "pug": "^3.0.0",
    "save": "^2.4.0",
    "sequelize": "^6.3.5",
